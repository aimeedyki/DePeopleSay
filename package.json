{
  "name": "depeoplesay",
  "version": "1.0.0",
  "description": "a fun srvey app",
  "engines": {
    "node": "12.4.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha server/tests/**/*.test.js --require babel-register --exit",
    "start:dev-server": "nodemon server/app.js --exec babel-node server/app.js",
    "start:server": "babel-node server/app.js",
    "start:client": "cd client && npm start",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:dev": "concurrently \"npm run start:dev-server\" \"npm run start:client\"",
    "heroku-postbuild": "NODE_ENV=production cd client && npm install && npm run build"
  },
  "author": "aimeedyki",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "dotenv": "^5.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.17.3",
    "faker": "^4.1.0",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.2.1",
    "mocha": "^5.1.1",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "save": "^2.4.0",
    "supertest": "^3.1.0"
  },
  "devDependencies": {
    "chai-subset": "^1.6.0",
    "nodemon": "^1.17.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
